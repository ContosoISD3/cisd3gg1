{
	"name": "Corgis_data_ingestion",
	"properties": {
		"activities": [
			{
				"name": "Ingest graduate data",
				"type": "Copy",
				"dependsOn": [
					{
						"activity": "Ingest school scores",
						"dependencyConditions": [
							"Succeeded"
						]
					}
				],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Corgis_binary",
						"type": "DatasetReference",
						"parameters": {
							"relativeURL": "/corgis/datasets/csv/graduates/graduates.csv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ADLS_binary",
						"type": "DatasetReference",
						"parameters": {
							"filesystem": "stage1np",
							"directory": "corgis2/graduates",
							"filename": "graduates.csv"
						}
					}
				]
			},
			{
				"name": "Ingest school scores",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "BinarySource",
						"storeSettings": {
							"type": "HttpReadSettings",
							"requestMethod": "GET"
						},
						"formatSettings": {
							"type": "BinaryReadSettings"
						}
					},
					"sink": {
						"type": "BinarySink",
						"storeSettings": {
							"type": "AzureBlobFSWriteSettings"
						}
					},
					"enableStaging": false
				},
				"inputs": [
					{
						"referenceName": "DS_Corgis_binary",
						"type": "DatasetReference",
						"parameters": {
							"relativeURL": "/corgis/datasets/csv/school_scores/school_scores.csv"
						}
					}
				],
				"outputs": [
					{
						"referenceName": "DS_ADLS_binary",
						"type": "DatasetReference",
						"parameters": {
							"filesystem": "stage1np",
							"directory": "corgis2/school_scores",
							"filename": "school_scores.csv"
						}
					}
				]
			}
		],
		"annotations": []
	}
}