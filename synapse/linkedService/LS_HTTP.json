{
	"name": "LS_HTTP",
	"properties": {
		"description": "Connection to an HTTP endpoint.\nThe base_url parameter must be passed in from the dataset that utilizes this linked service.",
		"parameters": {
			"base_url": {
				"type": "string"
			}
		},
		"annotations": [],
		"type": "HttpServer",
		"typeProperties": {
			"url": "@{linkedService().base_url}",
			"enableServerCertificateValidation": true,
			"authenticationType": "Anonymous"
		},
		"connectVia": {
			"referenceName": "AutoResolveIntegrationRuntime",
			"type": "IntegrationRuntimeReference"
		}
	}
}